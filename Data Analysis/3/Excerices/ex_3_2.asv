% log L = n log l - l Sum x_i 
% d log L / d l = n / l - Sum x_i = 0 
% l = 1/mu


function mu_lambda_avg = ex_3_2 ()
    M = 50;
    n = 70;
    lambda = 4;
   
    for sample=1:M
        for i=1:n
            S(sample, i) = poissrnd(lambda);
        end
    end

    S = S';

    mu_lambda = sum(S)/n;
    mu_lambda_avg = sum(mu_lambda)/M
    hist(mu_lambda, 300);
end